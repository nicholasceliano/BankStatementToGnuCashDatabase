{
    "name": "bankstatementtognucashdatabase",
    "version": "1.0.0",
    "description": "Insert Bank Statement to GnuCash SQL database",
    "main": "index.js",
    "scripts": {
        "dev": "nodemon",
        "build": "rimraf ./build && tsc",
        "prod": "npm run build && node build/index.js",
        "lint": "eslint %cd%/src/**",
        "test": "mocha -w",
        "test-coverage": "nyc mocha && open-cli ./coverage/index.html"
    },
    "nyc": {
        "exclude": [
            "**/*.spec.ts",
            "build",
            "coverage",
            "src/environments"
        ],
        "reporter": [
            "html"
        ],
        "all": true
    },
    "author": "Nicholas Celiano",
    "license": "ISC",
    "dependencies": {
        "csv-parse": "^4.8.6",
        "inversify": "^5.0.1",
        "mysql": "^2.18.1",
        "node": "^13.7.0",
        "reflect-metadata": "^0.1.13",
        "uuid": "^3.4.0"
    },
    "devDependencies": {
        "@types/chai": "^4.2.9",
        "@types/mocha": "^7.0.1",
        "@types/mysql": "^2.15.8",
        "@types/node": "^13.7.4",
        "@types/uuid": "^3.4.7",
        "@typescript-eslint/eslint-plugin": "^2.20.0",
        "@typescript-eslint/parser": "^2.20.0",
        "chai": "^4.2.0",
        "eslint": "^6.8.0",
        "eslint-config-google": "^0.14.0",
        "mocha": "^7.0.1",
        "nodemon": "^2.0.2",
        "nyc": "^15.0.0",
        "opn-cli": "^5.0.0",
        "rimraf": "^3.0.2",
        "ts-node": "^8.6.2",
        "typescript": "^3.8.2"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nicholasceliano/BankStatementToGnuCashDatabase.git"
    },
    "bugs": {
        "url": "https://github.com/nicholasceliano/BankStatementToGnuCashDatabase/issues"
    },
    "homepage": "https://github.com/nicholasceliano/BankStatementToGnuCashDatabase#readme"
}
